    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env":{
                "DB_URL": "postgresql://postgres:mysecretpassword@localhost:5433/postgres",
                "API_FETCHER_URL" : "http://127.0.0.1:5000/"
            }
        }
    ]
}